<h1>Session#new</h1>
<p>Find me in app/views/session/new.html.erb</p>
<hr>
<% if flash[:message]%>
    <h2>!! <%=flash[:message]%> </h2>
<%end%>
<%= form_tag '/sessions', method: :post do %>
    <%= label_tag :username%>
    <%= text_field_tag :email%>
    <%= label_tag :password%>
    <%= password_field_tag :password%>
    <%= submit_tag "LOGIN"%>
<%end%>
